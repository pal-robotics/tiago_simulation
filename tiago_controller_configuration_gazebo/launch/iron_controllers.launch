<launch>
  <arg name="public_sim"     default="false"/>
  <arg name="tf_prefix" default=""/>

  <include file="$(find tiago_controller_configuration_gazebo)/launch/no_arm_controllers.launch">
    <arg name="tf_prefix" value="$(arg tf_prefix)"/>
  </include>
  <include file="$(find tiago_controller_configuration_gazebo)/launch/imu_controller.launch"/>

  <!-- Play motion -->
  <include file="$(find tiago_bringup)/launch/play_motion.launch">
    <arg name="robot_sufix" value="_iron" />
  </include>

  <!-- Motion planning -->
  <include file="$(find tiago_moveit_config)/launch/move_group.launch">
    <arg name="robot" value="iron" />
    <arg name="robot_sufix" value="_iron" />
  </include>
</launch>
